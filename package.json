{
  "name": "jthem",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "format": "prettier --write 'src/**/*.{js,ts,scss}'",
    "type-check": "tsc --noEmit",
    "optimize-svg": "svgo -f src/images/svg/icons -o src/images/svg/optimized",
    "generate-sprite": "svg-sprite-generate -d src/images/svg/optimized -o src/images/sprite.svg",
    "svg-process": "node core/scripts/svg-process.js",
    "prebuild": "npm run svg-process",
    "generate-iconfont": "node core/scripts/generate-iconfont.js"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/jquery": "^3.5.29",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "glob": "^10.3.10",
    "postcss": "^8.5.3",
    "postcss-url": "^10.1.3",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "svg-sprite": "^2.0.4",
    "svg-sprite-generator": "^0.0.7",
    "svgo": "^3.3.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "webfont": "^11.2.26"
  },
  "dependencies": {
    "gsap": "^3.13.0",
    "jquery": "^3.7.1",
    "simplex-noise": "^3.0.1",
    "svgtofont": "^6.3.2",
    "swiper": "^11.0.5",
    "tailwind-bootstrap-grid": "^5.1.0",
    "terser": "^5.39.0",
    "typed.js": "^2.1.0",
    "vite-plugin-static-copy": "^2.3.1"
  }
}
